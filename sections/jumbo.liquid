    <!--START OF jumbo-->

    <section class="jumbo">
        <div class="container">
          {%if section.settings.jumbo_main_img != blank%}
            <img class="jumbo-img" src="{{section.settings.jumbo_main_img | img_url:'400x'}}" style="background-size: cover; background-position: center; background-repeat: no-repeat;">
          {%else%}
            <img src="{{"jumbo-main-img.jpg"|asset_url}}" class="jumbo-img">
          {%endif%}
            <div class="jumbo-content-wrap">
                <h1 class="jumbo-title">{{section.settings.jumbo_title}}</h1>
                <h1 class="jumbo-title">{{section.settings.jumbo_sub_title}}</h2>  
                <a href="{{ routes.all_products_collection_url }}" class="jumbo-btn">View Products</a>
            </div>
            <div class="bottom-imgs-wrap-mobile hide">
                <div class="bottom-imgs-relative">
                    <div class="bottom-imgs-overlay"></div>
                    <img src="imgs/jumbo-bottom-1.jpg" class="bottom-imgs" alt="">
                    <img src="imgs/bottom-jumbo-2.jpg" class="bottom-imgs" alt="">
                    <img src="imgs/bottom-jumbo-3.jpg" class="bottom-imgs" alt="">
                </div>
            </div>
        </div>
        <div class="bottom-imgs-wrap">
            <div class="bottom-imgs-relative">
                <div class="bottom-imgs-overlay"></div>
                {% for block in section.blocks %}
                  {%if block.settings.jumbo_bottom_imgs != blank%}
                    <img src="{{block.settings.jumbo_bottom_imgs | img_url:'220x'}}" class="bottom-imgs">
                  {%else%}
                    <img src="{{"hero-bottom-1.jpg"|asset_url}}">
                  {%endif%}
                {%endfor%}
            </div>
        </div>
    </section>




